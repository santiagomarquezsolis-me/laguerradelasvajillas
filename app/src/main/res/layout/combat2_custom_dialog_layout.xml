<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="fill_parent"    android:layout_height="fill_parent"    android:background="@drawable/background02"    android:gravity="top" >    <LinearLayout        android:id="@+id/linearLayout2"        android:layout_width="fill_parent"        android:layout_height="wrap_content"        android:layout_alignParentLeft="true"        android:layout_alignParentTop="true"        android:layout_marginTop="5dp" >        <ImageView            android:id="@+id/actor_ImageView"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginBottom="5dp"            android:src="@drawable/actor_bot" />    </LinearLayout>    <LinearLayout        android:id="@+id/linearLayout1"        android:layout_width="fill_parent"        android:layout_height="wrap_content"        android:layout_alignParentLeft="true"        android:layout_below="@+id/linearLayout2"        android:layout_marginBottom="10dp"        android:orientation="vertical" >        <TextView            android:id="@+id/actor_TextView"            android:layout_width="wrap_content"            android:layout_height="0dp"            android:layout_gravity="bottom"            android:layout_marginLeft="10dp"            android:layout_weight="1"            android:text="Nivel de Energía (Guantebot):"            android:textAppearance="?android:attr/textAppearanceMedium"            android:textColor="#fcdf2b" />        <TextView            android:id="@+id/percenWater_TextView"            android:layout_width="fill_parent"            android:layout_height="0dp"            android:layout_gravity="center_vertical"            android:layout_marginLeft="10dp"            android:layout_weight="1"            android:text="100%"            android:textAppearance="?android:attr/textAppearanceMedium"            android:textColor="#fcdf2b" />        <ProgressBar            android:id="@+id/darthwater_ProgressBar"            style="?android:attr/progressBarStyleHorizontal"            android:layout_width="fill_parent"            android:layout_height="0dp"            android:layout_weight="1"            android:max="100"            android:progress="100" />        <TextView            android:id="@+id/textView1"            android:layout_width="wrap_content"            android:layout_height="0dp"            android:layout_weight="1"            android:text="Lanza los dados:"            android:textAppearance="?android:attr/textAppearanceMedium"            android:textColor="#fcdf2b" />    </LinearLayout>    <LinearLayout        android:id="@+id/linearLayout3"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentLeft="true"        android:layout_below="@+id/linearLayout1" >        <ImageView            android:id="@+id/water01_ImageView"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:src="@drawable/dado01" />        <ImageView            android:id="@+id/water02_ImageView"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginLeft="5dp"            android:src="@drawable/dado02" />        <Button            android:id="@+id/attack_Button"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginLeft="20dp"            android:background="@drawable/boton"            android:text="Atacar"            android:textColor="#ffffff"            android:textStyle="bold" />        <ImageView            android:id="@+id/martin01_ImageView"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginLeft="20dp"            android:src="@drawable/dado01" />        <ImageView            android:id="@+id/martin02_ImageView"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginLeft="5dp"            android:src="@drawable/dado02" />    </LinearLayout>    <LinearLayout        android:id="@+id/linearLayout4"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentLeft="true"        android:layout_alignParentRight="true"        android:layout_below="@+id/linearLayout3"        android:layout_marginTop="10dp"        android:orientation="vertical" >        <ImageView            android:id="@+id/imageView3"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:src="@drawable/actor_you" />        <TextView            android:id="@+id/textView2"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="Nivel de Energía (Tuyo):"            android:textAppearance="?android:attr/textAppearanceMedium"            android:textColor="#fcdf2b" />        <TextView            android:id="@+id/percenMartin_TextView"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="100%"            android:textAppearance="?android:attr/textAppearanceMedium"            android:textColor="#fcdf2b" />        <ProgressBar            android:id="@+id/martin_ProgressBar"            style="?android:attr/progressBarStyleHorizontal"            android:layout_width="fill_parent"            android:layout_height="0dp"            android:layout_weight="1"            android:max="100"            android:progress="100" />    </LinearLayout>    <LinearLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentLeft="true"        android:layout_below="@+id/linearLayout4"        android:orientation="vertical" >        <TextView            android:id="@+id/textView3"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="Resultado:"            android:textAppearance="?android:attr/textAppearanceMedium"            android:textColor="#00ff00" />        <TextView            android:id="@+id/combatDescription_TextView"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="La pelea no ha comenzado..."            android:textAppearance="?android:attr/textAppearanceMedium"            android:textColor="#ffffff" />    </LinearLayout></RelativeLayout>